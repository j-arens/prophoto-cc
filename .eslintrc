{
    "root": true,
    "plugins": ["react"],
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "globals": {
      "wp": true
    },
    "env": {
      "es6": true,
      "browser": true,
      "jquery": true,
      "node": true
    },
    "rules": {
      "no-debugger": "warn",
      "no-console": "warn",
      "react/prop-types": 0
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    }
  }